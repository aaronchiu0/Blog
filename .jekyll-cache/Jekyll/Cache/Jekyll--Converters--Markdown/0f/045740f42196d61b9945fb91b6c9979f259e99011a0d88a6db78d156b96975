	\newcommand{\differtiate}[2]{\: \dfrac{\strut\mathrm{d}}{\strut\mathrm{d{#2}}}{\left(#1\right)}}
	\newcommand{\differtial}[1]{\: \mathrm{d}{#1}}
	\newcommand{\vect}[1]{\boldsymbol{#1}}
